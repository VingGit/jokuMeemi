                    "li": [
                      {
                        "@class": "sc-djWYAM ifbBJP",
                        "#text": "Naton pääsihteeri Mark Rutten odotetaan ehdottavan kaksiosaista mallia puolustusmenojen nostamiseksi Yhdysvaltojen vaatimusten mukaiseksi."
                      },
                      {
                        "@class": "sc-djWYAM ifbBJP",
                        "#text": "Malli sisältää 3,5 prosentin menot puolustukseen ja 1,5 prosenttia muihin turvallisuuskustannuksiin."
                      },
                      {
                        "@class": "sc-djWYAM ifbBJP",
                        "#text": "Puolustusvaliokunnan puheenjohtaja uskoo, että Suomi voi helposti saavuttaa 1,5 prosentin osuuden nykyisillä kustannuksilla."
                      }
                    ]
                  },
                  "p": {
                    "@class": "sc-kiMgGE sc-bhgfbr eQvHnw iEywHx yle__article__paragraph",
                    "em": {
                      "@class": "sc-bLOsqA bMNXIX yle__article__emphasis",
                      "#text": "Tämä on tekoälyn avulla tuotettu, toimittajan tarkistama tiivistelmä."
                    }
                  }
                }
              },
              {
